<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .important {
    	font-weight: bold;
    }
  </ui:style>
  <g:HTMLPanel>
    <g:Grid>
    	<g:row>
    		<g:customCell>
    			<g:Label ui:field="summaryComment" text="Sehr gut, Du hast 9 von 10 Wörtli richtig eingegeben!"/>
    		</g:customCell>
    	</g:row>
    	<g:row>
    		<g:customCell>
    			<g:Button ui:field="btnOverview" text="Zurück zur Übersicht"/>
    		</g:customCell>
    	</g:row>
    </g:Grid>
  </g:HTMLPanel>
</ui:UiBinder> 